{
    "version":2,
    "builds":[{
    "src":"./index.js",
    "use":"@vercel/node"}
 
    ],
    "routes":[
        {
        "src":".(.*)",
        "dest":"index.js"
    },
    {
        "src": "/api",
        "dest": "server/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "server/index.js"
      }
    ]
    }